import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        
        for(int i = 1; scan.hasNext()== true; i++){
            String str = scan.nextLine();
            System.out.println(i + " " + str);}
    }
}
